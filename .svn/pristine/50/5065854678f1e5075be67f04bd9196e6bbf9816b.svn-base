  a.tooltips span {
	position: absolute;
	width:200px;
	height:auto;
	line-height: 30px;
	text-align: center;
	visibility: hidden;
	border-radius: 6px;
  }
  a.tooltips span:after {
	content: '';
	position: absolute;
	top: 100%;
	left: 50%;
	margin-left: -8px;
	width: 0; height: 0;
	border-top: 7px solid;
	border-right: 7px solid;
	border-left: 7px solid;
  }
  a:hover.tooltips span {
	visibility: visible;
    opacity: 1;
    bottom: 40px;
    left: 50%;
    margin-left: -100px;
    z-index: 999;
  }



